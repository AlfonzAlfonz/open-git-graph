{
	"name": "open-git-graph",
	"description": "swag",
	"version": "0.0.1",
	"scripts": {
		"vscode:prepublish": "pnpm run build",
		"build": "node build.mjs",
		"build:prod": "node build.mjs -p",
		"watch": "pnpm build --watch",
		"lint": "eslint src --ext ts && prettier . -c && tsc --noEmit"
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^5.0.0",
		"@rollup/plugin-commonjs": "^25.0.4",
		"@rollup/plugin-node-resolve": "^15.2.1",
		"@rollup/plugin-replace": "^5.0.2",
		"@rollup/plugin-swc": "^0.2.0",
		"@swc/core": "^1.3.85",
		"@tanstack/query-sync-storage-persister": "^5.0.5",
		"@tanstack/react-query": "^5.0.5",
		"@tanstack/react-query-persist-client": "^5.0.5",
		"@types/jest": "^29.5.4",
		"@types/lodash-es": "^4.17.10",
		"@types/mocha": "^10.0.1",
		"@types/node": "~20.11.16",
		"@types/react": "^18.2.28",
		"@types/react-dom": "^18.2.13",
		"@types/react-window": "^1.8.6",
		"@types/uuid": "^9.0.5",
		"@types/vscode": "^1.81.0",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"@vscode/codicons": "^0.0.33",
		"@vscode/test-electron": "^2.3.4",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.47.0",
		"eslint-plugin-prettier": "^5.0.0",
		"glob": "^10.3.3",
		"jest": "^29.6.4",
		"lodash-es": "^4.17.21",
		"mocha": "^10.2.0",
		"nodemon": "^3.0.1",
		"postcss": "^8.4.29",
		"postcss-import": "^15.1.0",
		"prettier": "^3.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-resizable-panels": "^0.0.55",
		"react-virtualized-auto-sizer": "^1.0.20",
		"react-window": "^1.8.9",
		"rollup": "^3.28.1",
		"tailwindcss": "^3.3.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.3.3",
		"uuid": "^9.0.1"
	},
	"displayName": "Open Git Graph",
	"engines": {
		"vscode": "^1.81.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/runtime.js",
	"extensionDependencies": [
		"vscode.git"
	],
	"contributes": {
		"menus": {
			"webview/context": [
				{
					"command": "open-git-graph.merge-head",
					"when": "webviewId == 'open-git-graph.graph' && (webviewSection == 'commit' || webviewSection == 'branch')",
					"group": "1_commit@1"
				},
				{
					"command": "open-git-graph.rebase-head",
					"when": "webviewId == 'open-git-graph.graph' && (webviewSection == 'commit' || webviewSection == 'branch')",
					"group": "1_commit@2"
				},
				{
					"command": "open-git-graph.reset-head",
					"when": "webviewId == 'open-git-graph.graph' && (webviewSection == 'commit' || webviewSection == 'branch')",
					"group": "1_commit@3"
				},
				{
					"command": "open-git-graph.checkout",
					"when": "webviewId == 'open-git-graph.graph' && webviewSection == 'branch'",
					"group": "2_branch@1"
				}
			]
		},
		"commands": [
			{
				"command": "open-git-graph.graph",
				"title": "Open Git Graph"
			},
			{
				"command": "open-git-graph.merge-head",
				"title": "Merge into current branch"
			},
			{
				"command": "open-git-graph.rebase-head",
				"title": "Rebase current branch on this commit"
			},
			{
				"command": "open-git-graph.reset-head",
				"title": "Reset current branch to this commit"
			},
			{
				"command": "open-git-graph.checkout",
				"title": "Checkout branch"
			}
		]
	}
}
